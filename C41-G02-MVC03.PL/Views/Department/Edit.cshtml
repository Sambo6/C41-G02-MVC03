@model Department
@{
    ViewData["Title"] = "Edit Department";
}

<h1>Edit Department</h1>
<form asp-action="Edit">

    <div asp-validation-summary="ModelOnly"></div>

    <partial name="PartialView/CreateEditDepartmentPartial" />

    <input type="submit" value="Update"  class="btn btn-success ml-2" /> 
    <a asp-controller="Department" asp-action="Index" class="btn btn-warning ml-2">Cancel</a>
</form>

@section ScriptValidations {
    <partial name="_ValidationScriptsPartial" />

}
